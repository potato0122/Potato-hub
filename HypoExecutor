local function NJMA_fake_script() -- OnOrOff.Shadows 
	local script = Instance.new('LocalScript', OnOrOff)

	if game.Lighting.GlobalShadows == false then
		script.Parent.Text = "Off"
	else
		script.Parent.Text = "On"
	end
end
coroutine.wrap(NJMA_fake_script)()
